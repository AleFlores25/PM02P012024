<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="PM02P012024.PageInit"
             Title="PageInit">
    <VerticalStackLayout>
        <Label 
            Text="DATOS GENERALES"
            VerticalOptions="Center" 
            HorizontalOptions="Center" />

        <Entry x:Name="nombres" Placeholder="Ingrese su nombre" Keyboard="Text"/>
        
        <Entry x:Name="apellidos" Placeholder="Ingrese sus apellidos" Keyboard="Text"/>

        <DatePicker x:Name="FechaNac" Format="d"/>

        <Entry x:Name="telefono" Placeholder="Ingrese su telefono" Keyboard="Telephone"/>

        <Button x:Name="btnIngresar" Text="Ingresar Datos" Clicked="btnIngresar_Clicked"/>

    </VerticalStackLayout>
</ContentPage>